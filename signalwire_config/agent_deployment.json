{
  "agent_configuration": {
    "name": "Easy Finance Loan Intake Agent",
    "description": "AI agent for loan application intake and 3CX transfer",
    "agent_endpoint": "https://your-app.com/agent/intake",
    "inbound_number": "+1-XXX-XXX-XXXX"
  },
  "voice_settings": {
    "voice": "en-US-Neural2-J",
    "speed": 1.0,
    "pitch": 0,
    "language": "en-US"
  },
  "call_settings": {
    "recording_enabled": true,
    "recording_format": "wav",
    "bridge_mode": "SIP_WEBRTC",
    "low_latency": true,
    "timeout_seconds": 600
  },
  "sip_configuration": {
    "trunk_endpoint": "your-twilio-sip-trunk.pstn.twilio.com",
    "authentication": {
      "username": "your-twilio-username",
      "password": "your-twilio-password"
    },
    "codec_preferences": ["PCMU", "PCMA", "G722"],
    "dtmf_detection": true
  },
  "transfer_settings": {
    "queue_a_did": "+1-XXX-XXX-XXXX",
    "queue_b_did": "+1-XXX-XXX-XXXX", 
    "transfer_announcement": "Please hold while I connect you with a senior underwriter.",
    "transfer_timeout": 30
  },
  "crm_integration": {
    "lookup_endpoint": "https://your-app.com/webhooks/signalwire/call-started",
    "update_endpoint": "https://your-app.com/webhooks/signalwire/intake-data",
    "transfer_endpoint": "https://your-app.com/webhooks/signalwire/transfer-request",
    "cleanup_endpoint": "https://your-app.com/webhooks/signalwire/call-ended"
  },
  "security": {
    "webhook_signature_validation": true,
    "allowed_origins": ["signalwire.com"],
    "rate_limiting": {
      "calls_per_minute": 60,
      "calls_per_hour": 1000
    }
  }
}